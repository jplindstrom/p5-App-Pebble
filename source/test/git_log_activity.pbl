# git log --date=short | pebble test/git_log_activity.pbl > test/git_log_activity.png
P::Regex->match( regex => qr/Date:   (\d+-\d+-\d+)/, has => "date" )
| ogroup_count { date => "commit_count" }
| osort { "date" }
| o { $_->date( S::DateTime->parse( $_->date ) ) }
| R::Chart::Time->render({
    x    => "date",
    y    => "commit_count",
    file => "test/git_log_activity.png",
})
